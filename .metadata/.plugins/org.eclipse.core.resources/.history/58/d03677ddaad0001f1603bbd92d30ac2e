package com.app.bitmanipulation;

import java.util.Scanner;

public class bitMaskSet {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int num=sc.nextInt();
		int pos=sc.nextInt();
		
		int bs=1<<pos;
		
	    int updnum=bs | num;
	    
	    System.out.println(updnum);
	}

}
