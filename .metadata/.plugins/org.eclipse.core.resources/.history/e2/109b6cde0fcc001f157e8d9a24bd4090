package com.app.arrays;

public class SortingArryas {

	public static void main(String[] args) {
		
		int arr[] = { 6, 4 , 8, 23, 10, 3, 2};
		
		public static void sortArryas(arr[])
		{
			for(int i=0; i<arr.length;i++)
			{
				if(arr[i]>arr[i++])
				{
					int temp=arr[i];
					arr[i]=arr[i++];
					arr[i]=temp;
					
				}
			}
			System.out.println(arr);
			
		}
		
		
		

	}

}
