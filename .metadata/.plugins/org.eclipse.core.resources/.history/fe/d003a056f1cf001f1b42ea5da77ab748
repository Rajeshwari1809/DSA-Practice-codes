package com.app.strings;

import java.util.Scanner;

public class Strings {

	
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		
		System.out.println("enter string ");
		String s1=sc.next();
		
		int n=s1.length();
		for(int i=0; i<=n+1 ;i++)
		{
			System.out.println(s1.charAt(i));
		}
		
		
		
		
	
		
	}
}
